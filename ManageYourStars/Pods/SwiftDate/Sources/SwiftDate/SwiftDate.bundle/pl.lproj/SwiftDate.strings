"colloquial_p_MM"					=	"%d minut temu";	    // minute,past,plural:		"3 minutes ago"

"colloquial_now"					=	"teraz";			    // less than 5 minutes if .allowsNowOnColloquial is set

"colloquial_n_0y"					=	"w tym roku";		    // this year
